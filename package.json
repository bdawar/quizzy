{
    "name": "quizzy",
    "version": "0.1.0",
    "private": true,
    "proxy": "http://localhost:3001/",
    "dependencies": {
        "react": "^16.2.0",
        "react-dom": "^16.2.0",
        "react-scripts": "1.1.0"
    },
    "scripts": {
        "api": "json-server --watch ./api/qBank.json --port 3009",
        "start-json": "concurrently --kill-others \"yarn run api\" \"react-scripts start\"",
        "start": "concurrently --kill-others \"nodemon server.js\" \"react-scripts start\"",
        "build": "react-scripts build",
        "test": "react-scripts test --env=jsdom",
        "eject": "react-scripts eject"
    },
    "devDependencies": {
        "concurrently": "^3.5.1",
        "json-server": "^0.12.1",
        "nodemon": "^1.14.12"
    }
}
